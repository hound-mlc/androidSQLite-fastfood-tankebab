        </activity>
        <activity android:name=".Login" ></activity>
        <activity android:name=".MenuInicial"></activity>
        <activity android:name=".Config"></activity>
        <activity android:name=".MenuPedido"></activity>
        <activity android:name=".PredCarta"></activity>
